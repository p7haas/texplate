{
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.intellisense.file.exclude": [
        "**/*.aux",
        "**/*.bbl",
        "**/*.bcf",
        "**/*.blg",
        "**/*.idx",
        "**/*.ind",
        "**/*.lof",
        "**/*.lot",
        "**/*.out",
        "**/*.toc",
        "**/*.acn",
        "**/*.acr",
        "**/*.alg",
        "**/*.glg",
        "**/*.glo",
        "**/*.gls",
        "**/*.ist",
        "**/*.fls",
        "**/*.log",
        "**/*.nav",
        "**/*.snm",
        "**/*.fdb_latexmk",
        "**/*.synctex.gz",
        "**/*.run.xml",
        "**/*.fls",
        "**/*.glg-abr",
        "**/*.glo-abr",
        "**/*.gls-abr",
        "**/*.lol",
        "**/*.xdy"
    ],
    "latex-workshop.bibtex-format.tab": "4 spaces",
    "latex-workshop.latex.search.rootFiles.include": [
        "texplate.tex"
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-lualatex",
                "-shell-escape",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk ðŸ”ƒ",
            "tools": [
                "latexmk"
            ]
        }
    ],
    "latex-workshop.latex.clean.fileTypes": [
        "*.aux",
        "*.bbl",
        "*.blg",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.lot",
        "*.out",
        "*.toc",
        "*.acn",
        "*.acr",
        "*.alg",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.fls",
        "*.log",
        "*.fdb_latexmk",
        "*.snm",
        "*.synctex(busy)",
        "*.synctex.gz(busy)",
        "*.nav",
        "*.fls",
        "*.glg-abr",
        "*.glo-abr",
        "*.gls-abr",
        "*.lol",
        "*.xdy",
        "*.bcf"
    ]
}